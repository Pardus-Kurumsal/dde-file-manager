/* ******************QSplitter****************** */
QSplitter{
    border:none;
}

QSplitter::handle {
    background-color:rgba(0, 0, 0, 0.1);
}

QSplitter::handle:horizontal {
    width: 1px;
}

QSplitter::handle:vertical {
    height: 1px;
}

/* ******************QSplitter****************** */

QPushButton#backButton{
    background-color:white;
    border-left: 1px solid rgba(0, 0, 0, 0.1);
    border-top: 1px solid rgba(0, 0, 0, 0.1);
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
    color: black;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    image: url(:/icons/images/icons/backward_normal.png);
    icon-size: 16px 16px;
}

QPushButton#backButton:hover{
    background-color:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                        stop: 0 #8CCFFF,
                        stop: 1  #4BB8FF);
    border: 1px solid #3caafd;
    color: white;
    image: url(:/icons/images/icons/backward_hover.png);
    icon-size: 16px 16px;
}

QPushButton#backButton:checked{
    border: 1px solid #2794ff;
    background-color:#2ca7f8;
}

QPushButton#backButton:disabled{
    background-color:white;
    border-left: 1px solid rgba(0, 0, 0, 0.1);
    border-top: 1px solid rgba(0, 0, 0, 0.1);
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
    image: url(:/icons/images/icons/backward_disabled.png);
}


QPushButton#forwardButton{
    background-color:white;
    border-top: 1px solid rgba(0, 0, 0, 0.1);
    border-right: 1px solid rgba(0, 0, 0, 0.1);
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
    color: black;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    image: url(:/icons/images/icons/forward_normal.png);
    icon-size: 16px 16px;
}

QPushButton#forwardButton:hover{
