/*TabBar*/
TabBar#TabBar{
    background-color: transparent;
    border: none;
}
/*TabBar tab */
TabBar>Tab#Tab{
    border-right: 1px solid rgba(0,0,0,30);
    border-bottom: 1px solid rgba(0,0,0,30);
    background-color: #fafafa;
    color: #949494;
    border-radius: 0px;
    font-size: 12px;
    margin-top: -2px;
}
TabBar>Tab#Tab:selected {
    border-right: 1px solid rgba(0,0,0,30);
    border-bottom: 1px solid rgba(0,0,0,30);
    background-color: #ffffff;
    color: #303030;
    border-radius: 0px;
    font-size: 12px;
}
TabBar>Tab#Tab:hover {
    border-right: 1px solid rgba(0,0,0,30);
    border-bottom: 1px solid rgba(0,0,0,30);
    background-color: #ededed;
    color: #303030;
    border-radius: 0px;
    font-size: 12px;
}
TabBar>Tab#Tab:!selected {
    margin-top: 0px; /* make non-selected tabs look smaller */
}
/* tabBar tab close button*/
DTabCloseButton#TabCloseButton{
    border:none;
    image: url(:/icons/images/icons/tab_close_normal.png);
